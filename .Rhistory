na_count <- as.vector(table(na))
network <- list(Outcomes = Outcomes, Treat = Treat, SE = SE, na = na, na_count = na_count)
if(is.null(V)){
network$V <- V
}
code <- contrast.network.rjags(network)
network$code <- code
class(network) <- "contrast.network.data"
return(network)
}
